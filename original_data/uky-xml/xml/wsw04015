  <dc:title>2BT20, 15BT20, Butler County, Kentucky. Annis Village. Structure Area II.</dc:title>
  <wpa:observer>R. D. Brown</wpa:observer>
  <wpa:project_reservoir>Late Prehistoric Mississippian Sites in Kentucky</wpa:project_reservoir>
  <wpa:date_photographed>1939-3-6</wpa:date_photographed>

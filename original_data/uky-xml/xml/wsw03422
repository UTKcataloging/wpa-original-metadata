  <dc:title>41JO14, 15JO14, Johnson County, Kentucky. Mayo. Feature #30.</dc:title>
  <wpa:observer>James C. Greenacre</wpa:observer>
  <wpa:project_reservoir>Late Prehistoric Fort Ancient Sites in Kentucky</wpa:project_reservoir>
  <wpa:date_photographed>1940-01-11</wpa:date_photographed>
